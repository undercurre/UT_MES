* {
  padding: 0;
  margin: 0;
}

html {
  width: 100%;
  height: 100%;
  overflow-y: hidden;
}

.container-fluid {
  padding-right: 15px;
  padding-left: 15px;
  margin-right: auto;
  margin-left: auto
}

.CollectProducts,
.PalletSweep,
.TransitCode,
.DockingStation,
.AutomaticPackaging,
.TranscodePassing,
.ProcessInspection {
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  overflow: hidden;
  color: white;
  font-family: 'Microsoft YaHei';
  background-color: #09203D;
  background-size: 100% 100%;
}

.container-fluid {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  padding: 0 15px;
}

.tit-btn {
  color: #00ffff;
  font-size: 15px;
  text-decoration: none;
  font-weight: bold;
  display: block;
  margin-bottom: 10px;
  line-height: 1.5;
}

.kanban-panel-bg {
  position: relative;
  border: 1px solid #2A6294;
}

.kanban-panel-bg span.border-span,
.kanban-wo-info-panel span.border-span {
  display: block;
  position: absolute;
  width: 35px;
  height: 35px;
}

.kanban-panel-bg span.border-span-m,
.kanban-wo-info-panel span.border-span-m {
  display: block;
  position: absolute;
  width: 16px;
  height: 16px;
}

.kanban-panel-bg span.top-left,
.kanban-wo-info-panel span.top-left {
  top: -4px;
  left: -4px;
  border-top: 3px solid #54dcf2;
  border-left: 3px solid #54dcf2;
}

.kanban-panel-bg span.top-right,
.kanban-wo-info-panel span.top-right {
  top: -4px;
  right: -4px;
  border-top: 3px solid #54dcf2;
  border-right: 3px solid #54dcf2;
}

.kanban-panel-bg span.bottom-left,
.kanban-wo-info-panel span.bottom-left {
  bottom: -4px;
  left: -4px;
  border-bottom: 3px solid #54dcf2;
  border-left: 3px solid #54dcf2;
}

.kanban-panel-bg span.bottom-right,
.kanban-wo-info-panel span.bottom-right {
  bottom: -4px;
  right: -4px;
  border-bottom: 3px solid #54dcf2;
  border-right: 3px solid #54dcf2;
}

.kanban-title {
  text-align: center;
  font-size: 25px;
  height: 90px;
  line-height: 90px;
  background: url('../../assets/images/head-bj.png') center center no-repeat;
  background-size: 100% 100%;
  margin-bottom: 10px;
}

.screen-span {
  height: 50px;
  width: 80px;
}

.kanban-title-panel {
  height: 100%;
  color: #8DC0FC;
}

.kanban-wo-info {
  text-align: center;
  font-size: 16px;
  padding: 10px 0 15px 0;
}

.block {
  width: 100%;
  height: calc((100vh - 40px - 122px) / 2 - 14px);
  box-sizing: border-box;
  margin-bottom: 15px;
  background: #123966;
}

.block-center {
  width: 100%;
  height: calc((100vh - 20px - 0px) / 2 - 14px);
  box-sizing: border-box;
  margin-bottom: 15px;
  background: #123966;
}

.block-center-content {
  padding: 12px;
  height: 100%;
  box-sizing: border-box;
}

.fullheight{
  height: calc(100vh - 105px);
}

.kanban-wo-info-panel {
  position: relative;
  border: 1px solid #54dcf2;
  width: 100%;
  height: 100%;
  line-height: 40px;
  height: 40px;
}

.kanban-row2 {
  height: 50%;
}

.kanban-wo-yield-panel {
  width: 100%;
  height: 100%;
}

#WoYieldPie {
  height: 100%;
  height: 100%;
}

.kanban-wo-site-panel {
  height: 100%;
}

#WoSiteBar {
  width: 100%;
  height: 100%;
  padding: 10px;
}

.kanban-row3 {
  height: 50%;
}

.kanban-andon-call-panel {
  height: 100%;
}

.kanban-panel-title {
  margin-top: 5px;
  font-size: 16px;
  font-weight: bold;
  color: white;
  padding-left: 10px;
  display: block;
  height: 50px;
}

.title-left {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 120px;
}

.title-right {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 90px;
}

.ListTable {
  width: 100%;
  margin: 0px;
  background-color: transparent;
  text-align: center;
  table-layout: fixed;
  font-size: 12px;
  margin-top: 12px;
  background: #37587D;
}

.ListTable .table>thead:first-child>tr:first-child>th {
  text-align: center;
  font-weight: bold;
}

.ListTable .table>thead>tr>th {
  vertical-align: bottom;
  border-bottom: none;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.ListTable .table>tbody>tr>td {
  border-top: none;
  vertical-align: middle;
}

.ListTable .table-striped>tbody>tr:nth-of-type(odd) {
  background-color: #2b2b5b;
}

.ListTable td {
  padding: 0px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.ListTable th {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.kanban-top-defect-panel {
  height: 100%;
}

#TopDefectBar {
  width: 100%;
  height: 100%;
  padding: 10px;
}

.kanban-hour-yield-panel {
  height: 100%;
}

#HourYieldBar,
#ExLineDateBar {
  width: 100%;
  height: 100%;
  padding: 10px;
}

.text-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
}

.kanban-wo-info .div-title {
  text-align: right;
  padding-right: 0;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.kanban-wo-info .div-value {
  text-align: left;
  padding-left: 0;
}

.swiper-pagination-bullet {
  background: #00ffff;
}

#TodayWorkOrderSummary::-webkit-scrollbar{
  display: none;
}
