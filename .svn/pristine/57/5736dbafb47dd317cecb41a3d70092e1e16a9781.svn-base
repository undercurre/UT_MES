import request from '@/plugin/axios'

// 首页视图
export function Index (query) {
  return request.get('SfcsProductComponents/Index', {
    params: query
  })
}

// 查询
export function LoadData (query) {
  return request.get('SfcsProductComponents/LoadData', {
    params: query
  })
}

// 获取附件表
export function GetComponentsAattachments (query) {
  return request.get('SfcsProductComponents/GetComponentsAattachments', {
    params: query
  })
}

// 获取替代料维护
export function GetSubstituteComponents (query) {
  return request.get('SfcsProductComponents/GetSubstituteComponents', {
    params: query
  })
}

// 保存
export function SaveData (data) {
  return request.post('SfcsProductComponents/SaveData', data)
}
