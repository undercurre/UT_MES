* {
  padding: 0;
  margin: 0;
}

html {
  width: 100%;
  height: 100%;
  overflow-y: hidden;
}

.SmtLine {
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  overflow: hidden;
  color: white;
  font-family: 'Microsoft YaHei';
  background-color: #040f3c;
  background-size: 100% 100%;
}

.container-fluid {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  padding: 0 15px;
}

.tit-btn {
  color: #00ffff;
  font-size: 15px;
  text-decoration: none;
  font-weight: bold;
  display: block;
  margin-bottom: 10px;
  line-height: 1.5;
}

.kanban-panel-bg {
  /*background-color: rgb(18, 41, 114);
    opacity: 0.8;*/
  position: relative;
  border: 1px solid #54dcf2;
}

.kanban-panel-bg span.border-span,
.kanban-wo-info-panel span.border-span {
  display: block;
  position: absolute;
  width: 35px;
  height: 35px;
}

.kanban-panel-bg span.border-span-m,
.kanban-wo-info-panel span.border-span-m {
  display: block;
  position: absolute;
  width: 16px;
  height: 16px;
}

.kanban-panel-bg span.top-left,
.kanban-wo-info-panel span.top-left {
  top: -4px;
  left: -4px;
  border-top: 3px solid #54dcf2;
  border-left: 3px solid #54dcf2;
}

.kanban-panel-bg span.top-right,
.kanban-wo-info-panel span.top-right {
  top: -4px;
  right: -4px;
  border-top: 3px solid #54dcf2;
  border-right: 3px solid #54dcf2;
}

.kanban-panel-bg span.bottom-left,
.kanban-wo-info-panel span.bottom-left {
  bottom: -4px;
  left: -4px;
  border-bottom: 3px solid #54dcf2;
  border-left: 3px solid #54dcf2;
}

.kanban-panel-bg span.bottom-right,
.kanban-wo-info-panel span.bottom-right {
  bottom: -4px;
  right: -4px;
  border-bottom: 3px solid #54dcf2;
  border-right: 3px solid #54dcf2;
}

.kanban-title {
  text-align: center;
  font-size: 25px;
  height: 70px;
  line-height: 70px;
  background: url('../../../assets/images/header_bg.png') center center no-repeat;
  background-size: cover;
}

.screen-span {
  height: 50px;
  width: 80px;
}

.kanban-title-panel {
  /*background-color: #FFFFCC;
    border-radius: 0 0 100px 100px;*/
  height: 100%;
  color: #00ffff;
  font-weight: bold;
}

.kanban-wo-info {
  text-align: center;
  font-size: 16px;
  padding: 10px 0 15px 0;
}

.block {
  width: 100%;
  /* height: calc((100vh - 25px - 45px) / 2 - 14px); */
  height: calc((100vh - 70px - 67px) / 2 - 14px);
  /* height: calc((100vh - 70px - 67px) / 3 - 15px); */
  box-sizing: border-box;
  margin-bottom: 15px;
}

.block-center {
  width: 100%;
  /* height: calc((100vh - 25px - 45px) / 2 - 14px); */
  height: calc((100vh - 70px - 65px) / 2 - 14px);
  box-sizing: border-box;
  margin-bottom: 15px;
}

.kanban-wo-info-panel {
  position: relative;
  border: 1px solid #54dcf2;
  width: 100%;
  height: 100%;
  line-height: 40px;
  height: 40px;
}

.kanban-row2 {
  height: 50%;
}

.kanban-wo-yield-panel {
  width: 100%;
  height: 100%;
  /*border-radius: 10px;*/
}

#WoYieldPie {
  height: 100%;
  height: 100%;
}

.kanban-wo-site-panel {
  height: 100%;
  /*border-radius: 10px;*/
}

#WoSiteBar {
  width: 100%;
  height: 100%;
  padding: 10px;
}

.kanban-row3 {
  height: 50%;
  /* padding: 20px 0; */
}

.kanban-andon-call-panel {
  height: 100%;
  /*border-radius: 10px;*/
}

.kanban-panel-title {
  /* line-height: 50px; */
  margin-top: 5px;
  font-size: 16px;
  font-weight: bold;
  color: white;
  padding-left: 10px;
  display: block;
  height: 50px;
}

.title-left {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 120px;
}

.title-right {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 90px;
}

.ListTable {
  /* height: 75%; */
  width: 100%;
  padding: 10px;
  margin: 0px;
  background-color: transparent;
  border-top: dotted 1px #54dcf2;
  text-align: center;
  table-layout: fixed;
  font-size: 12px;
}

.ListTable.table>thead:first-child>tr:first-child>th {
  text-align: center;
  font-weight: bold;
}

.ListTable.table>thead>tr>th {
  vertical-align: bottom;
  border-bottom: none;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  /*border-bottom: dotted 1px #eee;*/
}

.ListTable.table>tbody>tr>td {
  border-top: none;
  vertical-align: middle;
}

.ListTable.table-striped>tbody>tr:nth-of-type(odd) {
  background-color: #2b2b5b;
}

.ListTable td {
  padding: 0px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.ListTable th {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.kanban-top-defect-panel {
  height: 100%;
  /*border-radius: 10px;*/
}

#TopDefectBar {
  width: 100%;
  height: 100%;
  padding: 10px;
}

.kanban-hour-yield-panel {
  height: 100%;
  /*border-radius: 10px;*/
}

#HourYieldBar,
#ExLineDateBar {
  width: 100%;
  height: 100%;
  padding: 10px;
}

.text-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
  /*inline-block*/
}

.kanban-wo-info .div-title {
  text-align: right;
  padding-right: 0;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.kanban-wo-info .div-value {
  text-align: left;
  padding-left: 0;
}

.swiper-pagination-bullet {
  background: #00ffff;
}