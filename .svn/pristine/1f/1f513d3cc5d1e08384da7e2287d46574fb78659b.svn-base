import request from '@/plugin/axios'

// 低水位预警 （柱状图和列表）
export function GetMesHiMaterialListenReelsModels (lineId) {
  return request.get('MesHiMaterialListen/GetMesHiMaterialListenReelsModels', {
    params: {
      lineId
    }
  })
}

